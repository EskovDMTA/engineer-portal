<h1> From partial </h1>
	<%= @buildings.each do |building| %>
		<div class="card mb-3">
  			<%= image_tag building.main_photo, class: "card-img-top"%>
  			<div class="card-body">
    			<h5 class="card-title"><%= building.title %></h5>
    			<p class="card-text"><%= building.developer %>, <%= building.material%></p>
    			<p class="card-text"><%= building.facing_area %></p>
    			<p class="card-text"><small class="text-muted"><%= building.updated_at %></small></p>
  			</div>
		</div>
<% end %>